<script>
    var TRACK_IN_GA = true,
        COHORT_YEAR = 0,
        COHORT_MONTH = 0,
        COHORT_WEEK = 0,
        METRICS_TAGS = [  ];

    function ga_track(events) {
        events.forEach(function (e) {
            if (e.value != undefined) ga('send', 'event', e.category, e.action, e.label, e.value);
            else if (e.label) ga('send', 'event', e.category, e.action, e.label);
            else ga('send', 'event', e.category, e.action);
        });
    }

    var lqStats = {
        NAVIGATION_EVENTS: 'Navigation Events',
