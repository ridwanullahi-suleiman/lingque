<script>

  var timeToBlocks = function(time){
    var str = time.toString();
    var strLength = str.length
    var strBlocks = ""
    var strIndex = ""

    for (let i = 0; i < strLength; i++) {
      strIndex = str.charAt(i)
      strBlocks += strLength == 1 ? `<span class="countdown-index-block">0</span><span class="countdown-index-block">${strIndex}</span>`: `<span class="countdown-index-block">${strIndex}</span>`
    }
    return (strBlocks)
}
// Set the date we're counting down to
  var countDownDate = new Date("Feb 18, 2024 23:59:59").getTime();

// Update the count down every 1 second
  var x = setInterval(function() {

    // Get today's date and time
    var now = new Date().getTime();

    // Find the distance between now and the count down date
    var distance = countDownDate - now;

    // Time calculations for days, hours, minutes and seconds
    var days = Math.floor(distance / (1000 * 60 * 60 * 24));
    var daysBlocks = timeToBlocks(days);

    var hours = Math.floor((distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
    //var hours = days * 24
    var hoursBlocks = timeToBlocks(hours);

    var minutes = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));
    var minutesBlocks = timeToBlocks(minutes);

    //var seconds = Math.floor((distance % (1000 * 60)) / 1000);
    //var secondsBlocks = timeToBlocks(seconds);

    // Display the result in the element with id="countdown-wrapper"
    document.getElementById("countdown-wrapper").innerHTML = daysBlocks+ ":" +hoursBlocks+ ":" +minutesBlocks;

    // If the count down is finished, write some text
    if (distance < 0) {
      clearInterval(x);
      document.getElementById("countdown-wrapper").innerHTML = "EXPIRED";
    }
  }, 10000);
</script>

