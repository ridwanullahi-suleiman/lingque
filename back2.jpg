        var d = document
        var moreLanguagesButton = d.getElementsByClassName("more-languages-dropdown")[0]
        var isActive
        var ua = navigator.userAgent,
            event = (ua.match(/iPad/i)) || (ua.match(/iPhone/i)) ? "touchstart" : "click";

        function toggleDropdown(){
            moreLanguagesButton.classList.toggle('is-active')
            isActive = moreLanguagesButton.classList.contains("is-active")
        }

        function trackSignupButtonClick(n) {
            return function () {
                ga('send', 'event', 'Signup Button Click', 'Homepage', 'Homepage ' + n);
            }
        }

        function trackingAppButtonClick(storeLabel, pagePosition){
            return function(){
              ga('send', {
                hitType: 'event',
                
                eventCategory: 'Home '+ pagePosition,
                
                eventAction: 'Landing App Button Clicks',
                eventLabel: storeLabel
              });
            }
        }
        
        d.querySelectorAll(".footer .appstore")[0].addEventListener("click", trackingAppButtonClick("iOS", "Bottom"))
        d.querySelectorAll(".footer .androidapp")[0].addEventListener("click", trackingAppButtonClick("Android", "Bottom"))



        d.addEventListener(event, function (e) {
            var isClickInside = moreLanguagesButton.contains(e.target)

            if (isClickInside == false && isActive == true) {
                moreLanguagesButton.classList.remove("open", "is-active")
                isActive = moreLanguagesButton.classList.contains("is-active")
            }
        })


        d.getElementsByClassName("header-signup-button")[0].addEventListener("click", trackSignupButtonClick(1))
        d.getElementsByClassName("signup-button2")[0].addEventListener("click", trackSignupButtonClick(2))
        d.getElementsByClassName("signup-button3")[0].addEventListener("click", trackSignupButtonClick(3))
        d.getElementsByClassName("signup-button4")[0].addEventListener("click", trackSignupButtonClick(4))
        d.getElementsByClassName("signup-button5")[0].addEventListener("click", trackSignupButtonClick(5))
        d.getElementsByClassName("signup-button6")[0].addEventListener("click", trackSignupButtonClick(6))
        d.getElementsByClassName("signup-button7")[0].addEventListener("click", trackSignupButtonClick(7))

        d.getElementsByClassName("more-languages-dropdown")[0].addEventListener("click",() => toggleDropdown())

    });

